# Docker Compose Environment Variables
# Copy this file to .env and fill in your values

# Telegram Configuration (Required)
TELEGRAM_BOT_TOKEN=your_bot_token_from_botfather
TARGET_LOGIN_URL=https://grp01.id.rakuten.co.jp/rms/nid/vc?__event=login&service_id=top

# Telegram Channel (Optional)
FORWARD_CHANNEL_ID=-1001234567890
ALLOWED_USER_IDS=123456789,987654321

# Batch Processing Configuration
BATCH_CONCURRENCY=1
BATCH_MAX_RETRIES=2
TIMEOUT_MS=60000

# Proxy Configuration (Optional - comma-separated)
PROXY_POOL=proxy1:port1,proxy2:port2

# Example values:
# TELEGRAM_BOT_TOKEN=1234567890:ABCdefGHIjklMNOpqrsTUVwxyz
# TARGET_LOGIN_URL=https://grp01.id.rakuten.co.jp/rms/nid/vc?__event=login&service_id=top
# FORWARD_CHANNEL_ID=-1001234567890
# ALLOWED_USER_IDS=123456789
# PROXY_POOL=proxy1.example.com:8080,proxy2.example.com:8080