# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Rakuten Login URL
TARGET_LOGIN_URL=https://login.account.rakuten.com/sso/authorize?client_id=rakuten_ichiba_top_web&service_id=s245&response_type=code&scope=openid&redirect_uri=https%3A%2F%2Fwww.rakuten.co.jp%2F

# Optional Settings
TIMEOUT_MS=60000
SCREENSHOT_ON=false
PROXY_SERVER=

# Batch processing
# BATCH_CONCURRENCY controls how many worker checks run in parallel (default: 8)
BATCH_CONCURRENCY=8

# HTTP Checker (Experimental)
# Set to 'true' to use HTTP-based checker instead of Puppeteer
# HTTP checker is faster and more concurrent but requires exact request replication
# USE_HTTP_CHECKER=false
